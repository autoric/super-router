{
"env" : {
  "node" : true,
  "browser" : true,
  "mocha" : true,
  "es6" : true
},
"globals" : {
  "angular" : true,
  "chai" : true
},
"ecmaFeatures" : {
  "modules" : true
},
"rules" : {
  "strict" : 0,
  "no-cond-assign" : 2,
  "no-debugger" : 2,
  "no-dupe-args" : 2,
  "no-dupe-keys" : 2,
  "no-duplicate-case" : 2,
  "no-ex-assign" : 2,
  "no-extra-semi" : 2,
  "no-func-assign" : 2,
  "no-invalid-regexp" : 2,
  "no-irregular-whitespace" : 1,
  "no-unreachable" : 2,
  "use-isnan" : 2,
  "valid-jsdoc" : [1, {
    "requireReturn" : true
  }],

  "complexity" : [1, 10],
  "consistent-return" : 2,
  "dot-notation" : 1,
  "eqeqeq" : [2, "smart"],
  "no-alert" : 2,
  "no-caller" : 2,
  "no-extend-native" : 2,
  "no-implied-eval" : 2,
  "no-invalid-this" : 0,
  "no-new" : 2,
  "no-param-reassign" : 1,
  "no-return-assign" : 1,
  "no-throw-literal" : 2,

  "no-delete-var" : 1,
  "no-undef" : 1,
  "no-unused-vars" : 1,
  "no-use-before-define" : [2, "nofunc"],

  "callback-return" : [1, ["callback", "cb", "next"]],
  "global-require" : 1,
  "handle-callback-err" : [2, "^(err|error)$" ],
  "no-path-concat" : 1,

  "array-bracket-spacing" : [1, "never"],
  "block-spacing" : [1, "always"],
  "brace-style" : [1, "stroustrup"],
  "camelcase" : 1,
  "comma-spacing" : [1, {"before" : false, "after" : true}],
  "eol-last" : 2,
  "func-style" : [1, "declaration"],
  "key-spacing" : [1, { "align" : "colon", "beforeColon" : true, "afterColon" : true }],
  "max-nested-callbacks" : [1, 3],
  "new-parens" : 2,
  "no-lonely-if" : 1,
  "no-mixed-spaces-and-tabs" : 2,
  "no-multiple-empty-lines" : [1, { "max" : 2 }],
  "no-nested-ternary" : 2,
  "no-spaced-func" : 2,
  "no-trailing-spaces" : [1, {"skipBlankLines" : true}],
  "no-underscore-dangle" : 0,
  "object-curly-spacing" : [1, "always"],
  "one-var" : [1, "never"],
  "quotes" : [2, "single"],
  "semi-spacing" : [1, {"before" : false, "after" : true}],
  "semi" : 2,
  "space-after-keywords" : 2,
  "space-before-blocks" : 2,
  "space-before-function-paren" : [2, {"anonymous" : "always", "named" : "never"}],
  "space-infix-ops" : 2,
  "spaced-comment" : [1, "always", { "exceptions" : ["-","+"] }],

  "arrow-spacing" : 2,
  "constructor-super" : 2,
  "generator-star-spacing" : [2, {"before" : false, "after" : true}],
  "no-class-assign" : 2,
  "no-const-assign" : 2,
  "no-dupe-class-members" : 2,
  "no-this-before-super" : 2,
  "no-var" : 2,
  "prefer-const" : 2,
  "prefer-spread" : 1,
  "prefer-template" : 1
  }
}
